<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:app="http://schemas.android.com/apk/res-auto"
            xmlns:tools="http://schemas.android.com/tools" android:id="@+id/nav_admin_graph"
            app:startDestination="@id/adminEmptyFragment">
    <fragment android:id="@+id/adminEmptyFragment" android:name="ru.ashesha.admintool.fragments.admin.InvisibleFragment"
              android:label="fragment_admin_empty" tools:layout="@layout/fragment_admin_empty">
        <action android:id="@+id/action_adminEmptyFragment_to_complaintFragment"
                app:destination="@id/complaintFragment"
        />
        <action android:id="@+id/action_adminEmptyFragment_to_banFragment" app:destination="@id/banFragment"/>
        <action android:id="@+id/action_adminEmptyFragment_to_unbanFragment" app:destination="@id/unbanFragment"/>
        <action android:id="@+id/action_adminEmptyFragment_to_playerFragment" app:destination="@id/playerFragment"/>
        <action android:id="@+id/action_adminEmptyFragment_to_roomFragment" app:destination="@id/roomFragment"/>
        <action android:id="@+id/action_adminEmptyFragment_to_findFragment" app:destination="@id/findFragment"/>
    </fragment>
    <fragment android:id="@+id/complaintFragment" android:name="ru.ashesha.admintool.fragments.admin.ComplaintFragment"
              android:label="fragment_complaint" tools:layout="@layout/fragment_complaint"/>
    <fragment android:id="@+id/banFragment" android:name="ru.ashesha.admintool.fragments.admin.BanFragment" android:label="fragment_ban"
              tools:layout="@layout/fragment_ban"/>
    <fragment android:id="@+id/unbanFragment" android:name="ru.ashesha.admintool.fragments.admin.UnbanFragment"
              android:label="fragment_unban" tools:layout="@layout/fragment_unban"/>
    <fragment android:id="@+id/playerFragment" android:name="ru.ashesha.admintool.fragments.admin.PlayerFragment"
              android:label="fragment_player" tools:layout="@layout/fragment_player"/>
    <fragment android:id="@+id/roomFragment" android:name="ru.ashesha.admintool.fragments.admin.RoomFragment"
              android:label="fragment_room" tools:layout="@layout/fragment_room"/>
    <fragment android:id="@+id/findFragment" android:name="ru.ashesha.admintool.fragments.admin.FindFragment"
              android:label="fragment_find" tools:layout="@layout/fragment_find"/>
</navigation>